{
  "name": "xana-react",
  "version": "1.1.0",
  "description": "Xana",
  "repository": "https://github.com/em-mubashir/xana-frontend",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@popperjs/core": "2.9.1",
    "@tailwindcss/forms": "0.2.1",
    "axios": "^0.23.0",
    "chart.js": "2.9.4",
    "chokidar": "^3.5.2",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.5",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "REACT_APP_ENV=local CHOKIDAR_USEPOLLING=true react-scripts start",
    "dev": "REACT_APP_ENV=development react-scripts start",
    "prod": "REACT_APP_ENV=production react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm start",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "typescript": "4.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.19"
  }
}
